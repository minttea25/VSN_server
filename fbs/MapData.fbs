namespace VSN;

struct Vector2Map {
  x: float;
  y: float;
}

struct SpawnSchedule {
  time: uint;
  type_id: ushort;
  position: Vector2Map;
}

table MapData {
  map_type: uint;
  map_size: Vector2Map;
  initial_player_spawnpoint: [Vector2Map];
  item_spawn_schedule: [SpawnSchedule];
  monster_spawn_schedule: [SpawnSchedule];
}

root_type MapData;
